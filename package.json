{
  "name": "autopwn",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:worker": "npm run dev --workspace=@autopwn/worker",
    "dev:web": "npm run dev --workspace=@autopwn/web",
    "build": "npm run build --workspaces",
    "clean": "rm -rf node_modules packages/*/node_modules packages/*/dist"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
