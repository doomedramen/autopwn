{
  "name": "autopwn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo test",
    "test:unit": "turbo test:unit",
    "test:integration": "turbo test:integration",
    "test:integration:real": "turbo test:integration:real",
    "test:e2e": "dotenv -e .env.test.local -- turbo test:e2e",
    "test:e2e:managed": "./scripts/test-e2e.sh",
    "test:e2e:managed:chromium": "./scripts/test-e2e.sh -p chromium",
    "test:e2e:managed:headed": "./scripts/test-e2e.sh --headed",
    "test:e2e:managed:headed:chromium": "./scripts/test-e2e.sh -p chromium --headed",
    "test:e2e:ui": "dotenv -e .env.test.local -- turbo test:e2e:ui",
    "test:e2e:debug": "dotenv -e .env.test.local -- turbo test:e2e:debug",
    "test:e2e:headed": "dotenv -e .env.test.local -- turbo test:e2e:headed",
    "test:e2e:chromium": "dotenv -e .env.test.local -- turbo test:e2e:chromium",
    "test:e2e:install": "turbo test:e2e:install",
    "test:e2e:report": "turbo test:e2e:report",
    "test:watch": "turbo test:watch",
    "test:coverage": "turbo test:coverage",
    "test:ci": "turbo test:ci && dotenv -e .env.test -- turbo test:e2e:chromium --reporter=json",
    "test:cleanup": "docker system prune -f",
    "test:performance": "turbo test:performance",
    "knip": "knip"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^20.19.9",
    "@vitest/coverage-v8": "^2.1.8",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "concurrently": "^9.2.1",
    "dotenv-cli": "^7.4.4",
    "dotenv-flow": "^4.1.0",
    "knip": "^5.66.3",
    "prettier": "^3.6.2",
    "testcontainers": "^10.17.0",
    "turbo": "^2.5.5",
    "typescript": "5.9.2",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  }
}
