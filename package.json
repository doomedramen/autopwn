{
  "name": "autopwn",
  "version": "1.0.0",
  "description": "Automated WPA/WPA2 handshake cracker with modern web dashboard",
  "private": true,
  "author": "AutoPWN Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DoomedRamen/autopwn.git"
  },
  "keywords": [
    "wifi",
    "wpa",
    "wpa2",
    "hashcat",
    "security",
    "penetration-testing",
    "password-cracking"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=@autopwn/web",
    "dev:backend": "turbo run dev --filter=@autopwn/backend",
    "build": "turbo run build",
    "build:shared": "turbo run build --filter=@autopwn/shared",
    "build:web": "turbo run build --filter=@autopwn/web",
    "build:backend": "turbo run build --filter=@autopwn/backend",
    "type-check": "turbo run type-check",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "clean:all": "rm -rf node_modules packages/*/node_modules packages/*/dist packages/*/.next .turbo",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:codegen": "playwright codegen",
    "test:e2e:install": "playwright install",
    "test:e2e:docker": "DOCKER=true playwright test",
    "test:e2e:ui:docker": "DOCKER=true playwright test --ui",
    "test:e2e:debug:docker": "DOCKER=true playwright test --debug",
    "test:e2e:codegen:docker": "DOCKER=true playwright codegen",
    "test:e2e:install:docker": "DOCKER=true playwright install",
    "test:unit": "turbo run test --filter=!@autopwn/web-e2e",
    "test": "pnpm run test:unit && pnpm run test:e2e",
    "test:report": "playwright show-report",
    "postinstall": "lefthook install"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "lefthook": "^1.13.6",
    "playwright": "^1.55.1",
    "turbo": "^2.2.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=9.0.0"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@types/pg": "^8.15.5",
    "better-auth": "^1.3.27",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.6",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "zod": "^4.1.12"
  }
}
