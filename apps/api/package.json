{
  "name": "@autopwn/api",
  "version": "1.0.0",
  "description": "AutoPWN API Server",
  "main": "src/index-minimal.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "worker": "tsx src/workers/index.ts",
    "worker:email": "tsx src/workers/email-worker.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed-superuser": "tsx src/db/seed-superuser.ts",
    "db:seed-config": "tsx src/db/seed-config.ts",
    "db:seed-all": "npm run db:seed-superuser && npm run db:seed-config",
    "queue:stats": "tsx -e \"import('./src/lib/queue').then(m => m.checkQueueHealth()).then(console.log)\"",
    "worker:health": "tsx -e \"import('./src/workers').then(m => m.checkWorkerHealth()).then(console.log)\""
  },
  "dependencies": {
    "@hono/node-server": "^1.19.5",
    "@hono/zod-validator": "^0.7.4",
    "@prisma/client": "^6.17.1",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^11.0.0",
    "@types/ws": "^8.18.1",
    "bcryptjs": "^3.0.2",
    "better-auth": "^1.4.10",
    "bullmq": "^5.61.0",
    "dotenv-flow": "^4.1.0",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.6",
    "drizzle-zod": "^0.8.3",
    "hono": "^4.11.3",
    "ioredis": "^5.8.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.12",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "prisma": "^6.17.1",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}
